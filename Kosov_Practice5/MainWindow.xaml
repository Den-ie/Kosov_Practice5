        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        <TextBox x:Name="FValue"
                 Margin="10,10,668,398"
                 />
        <TextBox x:Name="SValue"
                 Margin="10,50,668,358"
                 />

        <TextBox x:Name="Answer"
